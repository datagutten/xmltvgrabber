{
    "name": "datagutten/xmltvgrabber",
    "description": "A tool to grab tv-listings and save them in xmltv format",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "datagutten",
            "email": "datagutten@datagutten.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "datagutten/tools": "^1.8",
        "datagutten/xmltvtools": "^3.6",
        "rmccue/requests": "^2.0",
        "symfony/process": "^5.3|^6"
    },
    "autoload": {
        "psr-4": {
            "datagutten\\xmltv\\grabbers\\": "grabbers",
            "datagutten\\xmltv\\tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "symfony/filesystem": "^4.3|^5.0|^6"
    },
   "config": {
     "sort-packages": true
   },
    "bin": ["grab.php"]
}
